<%= form_with do |f| %>
    <div class="form-section">  
        <%= f.label :email %>
        <%= f.email_field :email %>
    </div>
    <div class="form-section">  
        <%= f.label :password %>
        <%= f.password_field :password %>
    </div>
    <%= f.submit "Login", class: "form-button"%>
<% end %>
