<h1>Login</h1>

<div class="card">

    <%= form_with do |f| %>  
        <%= f.label :email %>
        <%= f.email_field :email %>
        <br>
        <br>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <br>
        <br>
        <%= f.submit %>
    <% end %>

</div>